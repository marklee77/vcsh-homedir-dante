[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/dante.git]
checkout = vcsh clone https://gitlab.stillwell.me/mark/vcsh-homedir-dante.git dante
setpush  = vcsh dante remote set-url --add --push origin git@stillwell.me:mark/vcsh-homedir-dante.git
           vcsh dante remote set-url --add --push origin git@github.com:marklee77/vcsh-homedir-dante.git
           vcsh dante remote set-url --add --push origin git@bitbucket.org:marklee77/vcsh-homedir-dante.git

